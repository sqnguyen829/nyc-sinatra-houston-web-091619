<h2>Edit Historical Landmark</h2>

<form action="/landmarks/<%=@landmark.id%>" method="post">
<input type="hidden" name="_method" value="patch">
<label> Landmark: </label>
<input type="text" name="name" value="<%= @landmark.name %>"/>
<label> Year Completed: </label>
<input type="text" name="year_completed" value="<%= @landmark.year_completed %>"/>
<input type="submit"/>
</form>

<a class="button" href="/figures">Figures</a>
<a class="button" href="/">Home</a>
